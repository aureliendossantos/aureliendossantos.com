---
import { getEntryBySlug } from "astro:content"
import TocSection from "./TocSection.astro"

interface Props {
	big?: boolean
}
const { big } = Astro.props

const capCuisine = await getEntryBySlug("portfolio", "cap-cuisine")
const dashboard = await getEntryBySlug("portfolio", "dashboard")
const faireDesJeux = await getEntryBySlug("portfolio", "fairedesjeux")
const jeuDeSurvie = await getEntryBySlug("portfolio", "jeu-de-survie-navigateur")
const cms = await getEntryBySlug("portfolio", "minimal-cms")
const powerup = await getEntryBySlug("portfolio", "powerup")
const qrpg = await getEntryBySlug("portfolio", "qrpg")
// TODO: const site = await getEntryBySlug("portfolio", "site")
---

<TocSection big={big} title="Web" entries={[dashboard, cms, jeuDeSurvie]} />
<TocSection big={big} title="Unity" entries={[powerup, qrpg]} />
